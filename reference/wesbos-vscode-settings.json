		"**/.DS_Store": true,
		"**/.git": true,
		"**/.hg": true,
		"**/.next": true,
		"**/.svn": true,
		"**/CVS": true,
		"**/dist": false
	},
	"files.insertFinalNewline": true,
	"files.trimTrailingWhitespace": true,
